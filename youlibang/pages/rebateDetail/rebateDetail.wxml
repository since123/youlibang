<!--pages/rebateDetail/rebateDetail.wxml-->
<view class='box'>
  <view class='upgrade'><image src='../../images/vipbgImg@2x.png'></image></view>
  <view class='yongjin'>累计收益</view>
  <view class='money'><label>￥</label>{{accumulatedIncome}}</view>
</view>

<view class='threeDiv'>
  <block wx:for="{{nameList}}" wx:key="item.index">
    <view class='listInfo'>
        <view class='userImg'><image src='{{item.image}}'></image></view>
        <view class='leftInfo'>
            <view class='lookdetail'>{{item.userName}}购买的订单已完成</view>
            <view class='rebatetime'>2019-08-09</view>
        </view>
        <view class='rebateMoney'><label>￥</label>{{item.income}}</view>
    </view>
    <view class='li'></view>
  </block>
</view>

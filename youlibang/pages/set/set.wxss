/* pages/set/set.wxss */
.buttonInfo {
  display: flex;
  background-color: #fff;
}
.buttonInfo button::after {
  border: none;
  
}
.buttonInfo button{
  flex: 1;
  position: relative;
  color:rgba(51,51,51,1); 
  text-align: left;
  background-color: #fff;
  font-size: 28rpx;
  line-height: 100rpx;
  height: 100rpx;
  margin: 0;
}

.listInfo{
  width: 750rpx;
  height: 100rpx;
  display: flex;
  background-color: #ffffff;
  margin-top: 2rpx;
}

.text{
  /* background-color: yellow; */
  flex: 1;
  margin-top: 1%;
  margin-left: 5%;
  font-size:30rpx;
  font-family:sans-serif;
  font-weight:500;
  color:rgba(51,51,51,1);
  line-height:90rpx;
}
.next{
  margin-right: 5%;
  width: 11rpx;
  height: 20rpx;
  /* background-color: red; */
  margin-top: 3%;
}
.next image{
  width: 11rpx;
  height: 20rpx;
}
.Logout{
  width: 100%;
  height: 88rpx;
  background-color: #ffffff;
  bottom: 0;
  position: absolute;
  font-size:36rpx;
  font-family:sans-serif;
  font-weight:500;
  color:rgba(51,51,51,1);
  line-height:70rpx;
  justify-content: center;
  align-items: center;
  display: flex;
}
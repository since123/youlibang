font-size: 24rpx;
border-left: 1rpx solid #cccccc;
border-right: 1rpx solid #cccccc;
}
/*普通样式*/
.stepper .normal {
color:black;
}
/*禁用样式*/
.stepper .disable {
color: #ccc;
}
